
PORT=3000
DB_HOST=localhost
DB_USER=sw2_user
DB_PASSWORD=sw2_password
DB_NAME=sw2_db
JWT_SECRET=your_jwt_secret
API_KEY=your_api_key_here

# PMS API Configuration (Cloudbeds/ResNexus)
PMS_API_URL=https://api.cloudbeds.com/v1.1
PMS_API_KEY=your_pms_api_key_here

# MEWS (demo/sandbox) configuration
USE_REAL_PMS=true
TEST_USE_REAL_PMS=true
PMS_PROVIDER=mews
# ClientToken
MEWS_CLIENT_ID=your_mews_client_id_here
# AccessToken
MEWS_CLIENT_SECRET=your_mews_client_secret_here
MEWS_TOKEN_URL=
MEWS_BASE_URL=https://api.mews-demo.com
MEWS_BOOKING_PATH=/connector/v1/reservations
MEWS_AVAILABILITY_PATH=/connector/v1/availability
MEWS_API_STYLE=connector_v1
MEWS_WEBHOOK_SECRET=your_mews_webhook_secret_here
MEWS_WEBHOOK_SIGNATURE_HEADER=mews-signature

# Redis configuration for BullMQ (optional)
REDIS_URL=redis://127.0.0.1:6379
USE_BULL=false

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
# Optional: a test card number you can use when creating PaymentMethods locally
STRIPE_VISA_TEST_CARD=4242424242424242
STRIPE_RETURN_URL=https://example.com/stripe-return
USE_REAL_STRIPE=true

# Extra charge percent (example business config)
EXTRA_CHARGE_PERCENT=5

# Security Configuration
VALID_API_KEYS=key1,key2,key3
PMS_ENCRYPTION_KEY=your-32-character-encryption-key-here-change-in-prod

# Environment
NODE_ENV=test