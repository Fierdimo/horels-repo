{
  "name": "sw2-backend",
  "version": "1.0.0",
  "description": "Backend for SW2 hospitality swap/conversion platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "test": "npm run test:sequential && npm run test:e2e",
    "test:e2e": "vitest run tests/e2e.test.ts",
    "test:ui": "vitest --ui",
    "test:sequential": "bash -lc 'mkdir -p tmp && ./scripts/run_tests_sequential.sh 2>&1 | tee tmp/sequential_test_results.txt; exit ${PIPESTATUS[0]}'",
    "test:sequential:manual": "bash ./scripts/manual_sequential.sh",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "start:worker": "ts-node src/bin/start-worker.ts",
    "worker:sync": "ts-node src/bin/worker-sync.ts",
    "check:mews": "ts-node src/scripts/check_mews_connection.ts",
    "clean-resync": "ts-node scripts/clean-and-resync.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.65.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.4.5",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.0",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.14",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "author": "",
  "license": "ISC"
}
